@import '../../assets/common.less';
.award{
	background:white;
	border-radius: 5px;
	width:60%;
	height:60%;
	min-width:300px;
	min-height: 500px;
	max-width: 800px;
	max-height: 800px;
	position: absolute;
	z-index: 2;
	.padfn();
	text-align: center;
	.close{
		cursor: pointer;
		&:hover{
			color:@red;
		}
	}
	.title{
		color:green;
	}
	h4{
		font-size:20px;
	}
	.erweima{
		width:280px;
		height:280px;
		border: 2px solid @red;
		.padfn();
		border-radius: 5px;
		margin: 0 auto 10px auto;
		
	}
	ul{
		width:280px;
		margin:0 auto;
		text-align: left;
		.padfn();
	}
	
}